<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent">

    <fragment
        android:name="com.veckon.android.easywm.app.main.TitleFragment"
        android:id="@+id/title_fragment"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1" />

    <fragment
        android:name="com.veckon.android.easywm.app.make.MakeImageActivity_temp$PlaceholderFragment"
        android:id="@+id/make_image_fragment"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="3" />

    <fragment
        android:name="com.veckon.android.easywm.app.make.ToolsFragment"
        android:id="@+id/tools_fragment"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="2" />

</LinearLayout>


